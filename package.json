{
  "name": "gaza-timemap-datasheet-server",
  "version": "1.0.0",
  "description": "Autonomous data ingestion pipeline and secure monitoring dashboard for Gaza Timemap.",
  "main": "dist",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon -w src --exec \"node ./node_modules/@babel/node/bin/babel-node.js src\"",
    "build": "env NODE_ENV=production npx babel src -d dist",
    "start": "node dist",
    "seed:categories": "node seed_categories.js",
    "seed:sources": "node seed_sources.js",
    "lint": "standard \"src/**/*.js\" \"test/**/*.js\"",
    "test-watch": "ava --watch",
    "test": "ava --verbose"
  },
  "keywords": [
    "timemap",
    "humanitarian-data",
    "forensic-architecture",
    "data-pipeline"
  ],
  "author": "Alejandro Barquero",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dotenv": "^6.1.0",
    "express": "^4.13.3",
    "express-graphql": "^0.6.12",
    "express-handlebars": "^4.0.4",
    "express-rate-limit": "^8.2.1",
    "googleapis": "^39.1.0",
    "graphql": "^0.13.2",
    "morgan": "^1.8.0",
    "mz": "^2.7.0",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.7.0",
    "object-hash": "^1.3.0",
    "ramda": "^0.25.0",
    "resource-router-middleware": "^0.6.0",
    "xlsx": "^0.16.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/node": "^7.28.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.28.5",
    "@babel/register": "^7.0.0",
    "ava": "1.0.0-beta.8",
    "cross-env": "^10.1.0",
    "nodemon": "1.18.7",
    "standard": "^12.0.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "require": [
      "@babel/register",
      "@babel/polyfill"
    ]
  }
}